{
  "name": "sayyes",
  "version": "0.9.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "build": "node ./scripts/build/prod.mjs",
    "build-dev": "rm -rf ./public && node -r dotenv/config ./scripts/build/dev.mjs",
    "build-class": "webpack --env=production --mode=production --config-name=class",
    "build-cms": "webpack --env=production --mode=production --config-name=cms",
    "build-crm": "webpack --env=production --mode=production --config-name=crm",
    "build-front": "webpack --env=production --mode=production --config-name=front",
    "build-lms": "webpack --env=production --mode=production --config-name=lms",
    "cron": "node ./src/cron.js",
    "cron-dev": "nodemon -r dotenv/config ./src/cron.js",
    "db-backup": "node --import ./loaders/register.js -r dotenv/config ./scripts/db/backup.mjs",
    "db-migrate": "node --import ./loaders/register.js -r dotenv/config ./scripts/db/migrate.mjs",
    "db-restore": "node --import ./loaders/register.js -r dotenv/config ./scripts/db/restore.mjs",
    "db-seed": "node --import ./loaders/register.js -r dotenv/config ./scripts/db/seed.mjs",
    "deploy": "git push prod master",
    "deploy-dev": "git push dev dev:master",
    "deploy-ftp": "node --import ./loaders/register.js ./scripts/deploy.js",
    "dev-class": "webpack --watch --env=development --mode=development --config-name=class",
    "dev-cms": "webpack --watch --env=development --mode=development --config-name=cms",
    "dev-crm": "webpack --watch --env=development --mode=development --config-name=crm",
    "dev-front": "webpack --watch --env=development --mode=development --config-name=front",
    "dev-lms": "webpack --watch --env=development --mode=development --config-name=lms",
    "lint": "eslint ./src",
    "prestart": "npm run build",
    "start": "node src/main.js",
    "start-dev": "nodemon -r dotenv/config ./src/main.js",
    "test": "mocha -r dotenv/config",
    "test:unit": "npm test -- -r ./test/helpers.js",
    "test:functional": "npm test -- -r ./test/functional/setup.js"
  },
  "dependencies": {
