{
  "name": "sayyes",
  "version": "0.20.0",
  "private": true,
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build/prod.js",
    "build:dev": "node --env-file=.env.dev ./scripts/build/prod.js",
    "build:local": "node --env-file=.env.local ./scripts/build/dev.js",
    "build:prod": "node --env-file=.env.prod ./scripts/build/prod.js",
    "container": "docker compose up --build --detach",
    "container:dev": "docker compose --env-file .env.dev up --build --detach",
    "container:local": "docker compose --env-file .env.local up --build --detach",
    "container:prod": "docker compose --env-file .env.prod up --build --detach",
    "container:test": "docker compose --env-file .env.test up --build --detach",
    "cron": "node --import ./loaders/register.cjs ./src/cron.js",
    "cron:local": "nodemon --import ./loaders/register.cjs --env-file=.env.local ./src/cron.js",
    "db-backup": "node --env-file=.env.local ./scripts/db/backup.js",
    "db-export": "node --env-file=.env.local ./scripts/db/export.js",
    "db-import": "node --env-file=.env.local ./scripts/db/import.js",
    "db-migrate": "node --env-file=.env.local ./scripts/db/migrate.js",
    "db-seed:dev": "node --import ./loaders/register.cjs ./scripts/db/seed.js",
    "db-seed:local": "node --import ./loaders/register.cjs --env-file=.env.local ./scripts/db/seed.js",
    "predeploy:dev": "npm run test-env \"./test/**/*.test.js\"",
    "deploy:dev": "git push dev dev:master && npm run build:dev && npm run ftp:dev",
    "predeploy:prod": "npm run test-env \"./test/**/*.test.js\"",
    "deploy:prod": "git push prod master",
    "ftp:dev": "node --env-file=.env.dev ./scripts/deploy/ftp.js",
    "ftp:prod": "node --env-file=.env.prod ./scripts/deploy/ftp.js",
    "lint": "eslint ./src",
    "prestart": "npm run build",
    "prestart:dev_disabled": "npm run db-seed:dev",
    "start": "node --import ./loaders/register.cjs ./src/main.js",
    "start:dev": "node --import ./loaders/register.cjs ./src/main.js",
    "start:local": "nodemon --import ./loaders/register.cjs --env-file=.env.local ./src/main.js",
    "test": "mocha",
    "test-env": "mocha -r ./test/_env/setup.js"
  },
